var sibling = ["An", "Joe", "Bob", "Andy"]
var parents = ["Don", "Poo", "Jerry", "Tom"]
sibling.sort()
parents.sort().reverse()
parents.concat(sibling).sort()
parents.concat(sibling).sort().reverse()

function name(targetName, targetArray){
  if(targetArray.indexOf(targetName)>-1){
    console.log("Name Found")
  }else {
    console.log("Name not Found")
  }
}
